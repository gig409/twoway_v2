{
  "name": "twoway_v1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "format": "prettier --write .",
    "lint": "eslint .",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "deploy": "fly deploy",
    "db:migrate": "fly ssh console -C 'npx prisma migrate deploy'",
    "db:seed": "fly ssh console -C 'npx prisma db seed'"
  },
  "dependencies": {
    "@conform-to/react": "^1.8.2",
    "@conform-to/zod": "^1.8.2",
    "@flydotio/litestream": "^1.0.1",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.12.0",
    "@react-router/node": "^7.7.0",
    "@react-router/serve": "^7.7.0",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.6",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.5"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/playwright-report",
    "/server-build"
  ],
  "devDependencies": {
    "@epic-web/config": "^1.20.1",
    "@faker-js/faker": "^9.9.0",
    "@flydotio/dockerfile": "^0.7.10",
    "@react-router/dev": "^7.7.0",
    "@tailwindcss/vite": "^4.1.4",
    "@types/eslint": "^9.6.1",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^9.31.0",
    "prettier": "^3.6.2",
    "prettier-plugin-sql": "^0.19.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.12.0",
    "tailwindcss": "^4.1.4",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": "@epic-web/config/prettier",
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "unrs-resolver"
  ],
  "dockerfile": {
    "litestream": true
  }
}
